{# -- Component Variables -- #}

{% set this = {
    video: params.section|default(null)
} %}

{# -- Component Template -- #}

{% if this.video %}
<figure class="c-video" data-scroll-trigger>

    {% include '_elements/video.twig' with {
        params: {
            video: this.video.video_url
        }
    } only %}

</figure>
{% endif %}
