{# -- Partial Variables -- #}
{# ----------------------------------------------------------------- #}
{% set this = {
    primary: primaryMenu|default(null),
    utility: utilityMenu|default(null),
    logo: globals.header_logo|default(null),
    buttons: globals.header_buttons|default(null)
} %}

{# -- Partial Template -- #}
{# ----------------------------------------------------------------- #}
{% if this.utility %}
    {% include '_components/nav-utility.twig' with {
        menu: this.utility.get_items
    } only %}
{% endif %}

{% if this.primary %}
    {% include '_components/nav-primary.twig' with {
        menu: this.primary.get_items,
        params: {
            logo: this.logo,
            buttons: this.buttons
        }
    } only %}
{% endif %}
